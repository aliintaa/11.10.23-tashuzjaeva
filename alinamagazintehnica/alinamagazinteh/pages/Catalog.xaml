<Page x:Class="alinamagazinteh.pages.Catalog"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:alinamagazinteh.pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Catalog">

    <Grid>
        <Grid Background="#FFDAF4F4">
            <Grid.RowDefinitions>
                <RowDefinition Height="29*"/>
                <RowDefinition Height="388*"/>
                <RowDefinition Height="33*"/>
            </Grid.RowDefinitions>
            <ScrollViewer HorizontalScrollBarVisibility="Disabled" HorizontalAlignment="Center" Grid.Row="1">
                <WrapPanel x:Name="catalWP" Grid.Row="1"></WrapPanel>
            </ScrollViewer>
        </Grid>
    </Grid>
</Page>
